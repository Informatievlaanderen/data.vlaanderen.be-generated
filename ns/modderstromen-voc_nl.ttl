  rdfs:isDefinedBy <https://test.data.vlaanderen.be/nsl/modderstromen>;
  rdfs:range <https://test.data.vlaanderen.be/nsl/modderstromen#SedimentvolumeObservatie> .

<https://test.data.vlaanderen.be/nsl/modderstromen#sedimentvolumeVoor> a owl:ObjectProperty;
  rdfs:label "sedimentvolumeVoor"@nl;
  rdfs:comment "Het sedimentvolume van een erosiepoel voor het onderhoud."@nl;
  rdfs:domain <https://test.data.vlaanderen.be/nsl/modderstromen#Erosiepoelonderhoud>;
  rdfs:isDefinedBy <https://test.data.vlaanderen.be/nsl/modderstromen>;
  rdfs:range <https://test.data.vlaanderen.be/nsl/modderstromen#SedimentvolumeObservatie> .

<https://test.data.vlaanderen.be/nsl/modderstromen#verantwoordelijke> a owl:ObjectProperty;
  rdfs:label "verantwoordelijke"@nl;
  rdfs:comment "De verantwoordelijke van een Erosiebestrijding."@nl,
