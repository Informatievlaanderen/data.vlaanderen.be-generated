  rdfs:comment "De dimensie van de Erosiepoel."@nl;
  rdfs:domain <https://test.data.vlaanderen.be/nsl/modderstromen#Erosiepoel>;
  rdfs:isDefinedBy <https://test.data.vlaanderen.be/nsl/modderstromen>;
  rdfs:range <https://data.vlaanderen.be/ns/sensoren-en-bemonstering#Dimensie> .

<https://test.data.vlaanderen.be/nsl/modderstromen#erosiepoelonderdeel> a owl:ObjectProperty;
  rdfs:label "erosiepoelonderdeel"@nl;
  rdfs:comment "Een onderdeel van een erosiepoel, zoals ingang en uitgang."@nl;
