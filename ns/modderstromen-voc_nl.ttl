  rdfs:subClassOf <http://def.isotc211.org/iso19156/2011/Observation#OM_Observation> .

<https://test.data.vlaanderen.be/nsl/modderstromen#Erosiepoelonderhoud> a owl:Class;
  rdfs:label "Erosiepoelonderhoud"@nl;
  rdfs:comment "Een onderhoud van een erosiepoel."@nl;
  rdfs:isDefinedBy <https://test.data.vlaanderen.be/nsl/modderstromen>;
  rdfs:subClassOf <https://test.data.vlaanderen.be/nsl/modderstromen#Onderhoud> .

<https://test.data.vlaanderen.be/nsl/modderstromen#Onderhoud> a owl:Class;
  rdfs:label "Onderhoud"@nl;
  rdfs:comment "Een onderhoud van een Erosiebestrijdingsmaatregel."@nl;
  rdfs:isDefinedBy <https://test.data.vlaanderen.be/nsl/modderstromen>;
