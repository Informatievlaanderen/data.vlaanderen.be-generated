  rdfs:isDefinedBy <https://test.data.vlaanderen.be/nsl/modderstromen>;
  rdfs:range <https://test.data.vlaanderen.be/nsl/modderstromen#Zone> .

<https://test.data.vlaanderen.be/nsl/modderstromen#beschermdeZone> a owl:ObjectProperty;
  rdfs:label "beschermde zone"@nl;
  rdfs:comment "De zone die beschermd wordt tegen bijvoorbeeld een modderstroom door de Erosiebestrijdingsmaatregel."@nl;
  rdfs:domain <https://test.data.vlaanderen.be/nsl/modderstromen#Erosiebestrijdingsmaatregel>;
  rdfs:isDefinedBy <https://test.data.vlaanderen.be/nsl/modderstromen>;
