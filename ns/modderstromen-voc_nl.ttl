<https://test.data.vlaanderen.be/nsl/modderstromen#SedimentvolumeObservatie> a owl:Class;
  rdfs:label "SedimentvolumeObservatie"@nl;
  rdfs:comment "De observatie van het sedimentvolume."@nl;
  rdfs:isDefinedBy <https://test.data.vlaanderen.be/nsl/modderstromen>;
