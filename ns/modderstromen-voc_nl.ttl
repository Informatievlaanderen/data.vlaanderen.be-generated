  rdfs:isDefinedBy <https://test.data.vlaanderen.be/nsl/modderstromen>;
  rdfs:range schemas:QuantitativeValue .

<https://test.data.vlaanderen.be/nsl/modderstromen#onderhoud> a owl:ObjectProperty;
  rdfs:label "onderhoud"@nl;
