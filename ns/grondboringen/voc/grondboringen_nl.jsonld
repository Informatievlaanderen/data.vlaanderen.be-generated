  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/grondboringen#Bekisting",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Bekisting"
      },
      "definition": {
        "nl": "Kenmerken van de afwerking van het boorgat in functie van de diepte."
      },
      "usage": {
        "nl": "Bv het gebruikte materiaal en de dikte ervan. Het verloop van de Bekisting wordt voorgesteld als een DiscreetCurvebereik, dwz dat ze verandert in discrete stappen (intervallen) in functie van de diepte."
      },
      "parents": [
        "http://def.isotc211.org/iso19123/2005/DiscreteCoverages#CV_DiscreteCurveCoverage"
      ]
    },
