  "classes": [
    {
      "@id": "https://test.data.vlaanderen.be/ns/bodemerosie#Erosiebestrijdingsmaatregel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Erosiebestrijdingsmaatregel"
      },
      "definition": {
        "nl": "Werken of natuurlijke elementen die bodemerosie bestrijden, zoals erosiepoelen en graszones."
      },
      "parents": [
        "http://www.w3.org/ns/sosa/FeatureOfInterest"
      ]
    },
    {
      "@id": "https://test.data.vlaanderen.be/ns/bodemerosie#Erosiebestrijdingsmaatregelonderdeel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Erosiebestrijdingsmaatregelonderdeel"
      },
      "definition": {
        "nl": "Deel van een Erosiebestrijdingsmaatregel."
      },
      "parents": [
        "http://www.w3.org/ns/sosa/Platform"
      ]
    },
    {
      "@id": "https://test.data.vlaanderen.be/ns/bodemerosie#Erosiepoel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Erosiepoel"
      },
      "definition": {
        "nl": "Erosiepoelen vangen afstromend (modder)water op en bufferen het tijdelijk zodat de meegevoerde modder kan bezinken. In een aarden dam met erosiepoel wordt water niet permanent gestockeerd. Bij een volgende regenbui moet er immers terug water kunnen worden opgevangen."
      },
      "parents": [
        "https://test.data.vlaanderen.be/ns/bodemerosie#Erosiebestrijdingsmaatregel"
      ]
    },
    {
      "@id": "https://test.data.vlaanderen.be/ns/bodemerosie#Erosiepoelonderdeel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Erosiepoelonderdeel"
      },
      "definition": {
        "nl": "Een onderdeel van een erosiepoel, zoals ingang en uitgang."
      },
      "parents": []
    },
    {
      "@id": "https://test.data.vlaanderen.be/ns/bodemerosie#Graszone",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Graszone"
      },
      "definition": {
        "nl": "Een zone met gras om bodemerosie te bestrijden."
      },
      "parents": [
        "https://test.data.vlaanderen.be/ns/bodemerosie#Erosiebestrijdingsmaatregel"
      ]
    },
    {
      "@id": "https://test.data.vlaanderen.be/ns/bodemerosie#Zone",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Zone"
      },
      "definition": {
        "nl": "Een gebied dat aan een Erosiebestrijdingsmaatregel ligt. Dit kan bijvoorbeeld het gebied zijn dat sediment en water aanvoert."
      },
      "parents": [
        "http://www.w3.org/ns/sosa/FeatureOfInterest"
      ]
    }
  ],
