<https://data.vlaanderen.be/doc/applicatieprofiel/datakwaliteit/kandidaatstandaard/2023-06-01#ConformiteitResultaatShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype xsd:boolean;
    shacl:description "Geeft aan of de data al dan niet conform is."@nl;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "geaccepteerd"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQualityResult#DQ_ConformanceResult.pass>
  ], [
    shacl:class <http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation#CI_Citation>;
    shacl:description "Verwijzing naar de specifiatie waartegen de data gevalueerd wordt."@nl;
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "specificatie"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQualityResult#DQ_ConformanceResult.specification>
  ], [
    shacl:datatype xsd:string;
    shacl:description "Verdere uitleg over de conformiteit van de data."@nl;
    shacl:maxCount 1;
    shacl:name "toelichting"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQualityResult#DQ_ConformanceResult.explanation>
  ];
  shacl:targetClass <http://def.isotc211.org/iso19157/2013/DataQualityResult#DQ_ConformanceResult> .

<https://data.vlaanderen.be/doc/applicatieprofiel/datakwaliteit/kandidaatstandaard/2023-06-01#EvaluatieMethodeShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype xsd:string;
    shacl:description "Beschrijving van de evaluatiemethode."@nl;
    shacl:maxCount 1;
    shacl:name "evaluatieMethodeBeschrijving"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQualityEvaluation#DQ_EvaluationMethod.evaluationMethodDescription>
  ], [
    shacl:class skos:Concept;
    shacl:description "Type van de evaluatiemethode."@nl;
    shacl:maxCount 1;
    shacl:name "evaluatieMethodeType"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQualityEvaluation#DQ_EvaluationMethod.evaluationMethodType>
  ], [
    shacl:class <http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation#CI_Citation>;
    shacl:description "Verwijzing naar de procedure die werd gevolgd."@nl;
    shacl:maxCount 1;
    shacl:name "evaluatieProcedure"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQualityEvaluation#DQ_EvaluationMethod.evaluationProcedure>
  ], [
    shacl:class <http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation#CI_Citation>;
    shacl:description "Verwijzing naar een document dat de methode en/of procedure beschrijft."@nl;
    shacl:name "referentieDoc"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQualityEvaluation#DQ_EvaluationMethod.referenceDoc>
  ], [
    shacl:datatype xsd:dateTime;
