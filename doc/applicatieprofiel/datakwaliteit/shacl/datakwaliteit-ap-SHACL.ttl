    shacl:description "Verwijzing naar een Kwaliteitselement waarvan dit element is afgekleid."@nl;
    shacl:name "afgeleidElement"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQuality_2#DQ_Element.derivedElement>
  ], [
