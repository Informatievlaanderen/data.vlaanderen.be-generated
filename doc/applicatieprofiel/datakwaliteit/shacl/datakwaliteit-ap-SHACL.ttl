    shacl:maxCount 1;
    shacl:name "standaloneKwaliteitRapportDetails"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQuality_2#DQ_Element.standaloneQualityReportDetails>
  ];
