    shacl:name "tijdstip"@nl;
    shacl:path <http://def.isotc211.org/iso19157/2013/DataQualityEvaluation#DQ_EvaluationMethod.dateTime>
  ];
  shacl:targetClass <http://def.isotc211.org/iso19157/2013/DataQualityEvaluation#DQ_EvaluationMethod> .

