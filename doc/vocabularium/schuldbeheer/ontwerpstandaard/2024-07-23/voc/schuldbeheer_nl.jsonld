  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Aanmaning",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Aanmaning"
      },
      "definition": {
        "nl": "Schriftelijk berichtgeving van een schuldeiser naar een schuldenaar om deze schuldenaar te herinneren aan een schuldvordering die niet is voldaan terwijl de uiterste betaaldatum van deze schuldvordering reeds verstreken is.  "
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Aanvaarding",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Aanvaarding"
      },
      "definition": {
        "nl": "Het formeel akkoord gaan met een afbetalingsplan"
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Afbetaling",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Afbetaling"
      },
      "definition": {
        "nl": "Betaling in het kader van een afbetalingsplan"
      },
      "parents": [
        "https://data.vlaanderen.be/ns/schuldbeheer#Betaling"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Afbetalingsplan",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Afbetalingsplan"
      },
      "definition": {
        "nl": "Een gestructureerd schema dat wordt gebruikt om een schuld af te lossen. "
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Afbetalingsplanonderdeel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Afbetalingsplanonderdeel"
      },
      "definition": {
        "nl": "Een afbetalingsplan bestaat uit verschillende onderdelen. Elk onderdeel specifieert een bedrag en tijdstip waarop dit bedrag ten laatste betaald moet zijn. "
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Afbetalingsplansaldo",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Afbetalingsplansaldo"
      },
      "definition": {
        "nl": "Het nog af te betalen bedrag op een bepaalde datum."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/schuldbeheer#Saldo"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Betaling",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Betaling"
      },
      "definition": {
        "nl": "De overdracht van geld van één of meerdere partij(en) naar een andere partij. "
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Betwisting",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Betwisting"
      },
      "definition": {
        "nl": "Het toewijzen van een betaald geldbedrag aan een schuldvordering. "
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Inkomst",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Inkomst"
      },
      "definition": {
        "nl": "Het bedrag dat een persoon ontvangt voor geleverde prestaties, beschikbaar gestelde goederen, land of vermogen, of als ondersteuning."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/schuldbeheer#Solvabiliteitsindicator"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#JuridischStatuut",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Juridisch statuut"
      },
      "definition": {
        "nl": "Een statuut dat rechtsgevolgen bepaalt. "
      },
      "parents": [
        "https://data.vlaanderen.be/ns/schuldbeheer#Solvabiliteitsindicator"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Krediet",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Krediet"
      },
      "definition": {
        "nl": "Financiële regeling waarbij een kredietverstrekker geld leent aan een persoon of organisatie met de belofte dat het geleende bedrag later zal worden terugbetaald, meestal met rente."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/schuldbeheer#Solvabiliteitsindicator"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Oninbaarheidsverklaring",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Oninbaarheidsverklaring"
      },
      "definition": {
        "nl": "Het aangeven dat een bepaalde schuldvordering als oninbaar wordt beschouwd. "
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Saldo",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Saldo"
      },
      "definition": {
        "nl": "Het bedrag dat overblijft als je het verschil berekent tussen twee grootheden, zoals inkomsten en uitgaven, kosten en opbrengsten, te betalen bedrag en betaald bedrag. Een saldo is een momentopname."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Schrapping",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Schrapping"
      },
      "definition": {
        "nl": "Het formeel beëindigen of annuleren van een overeengekomen betalingsregeling, bijvoorbeeld omdat de schuldenaar zijn betaalverplichtingen niet naleeft."
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Schuldvordering",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Schuldvordering"
      },
      "definition": {
        "nl": "Aanspraak van een partij (de schuldeiser) op een bepaalde prestatie vanwege een andere partij (de schuldenaar)."
      },
      "usage": {
        "nl": "Voor de doeleinden van dit applicatieprofiel is de opgelegde prestatie altijd de betaling van een geldsom. "
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Schuldvorderingscategorie",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Schuldvorderingscategorie"
      },
      "definition": {
        "nl": "Een soort schuldvordering met bepaalde kenmerken."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Schuldvorderingssaldo",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Schuldvorderingssaldo"
      },
      "definition": {
        "nl": "Het verschil tussen het totale verschuldigde bedrag (hoofdsom en eventuele wanbetalingsvergoedingen) en het totale betaalde bedrag dat aan de schuldvordering werd toegerekend."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/schuldbeheer#Saldo"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#SociaalStatuut",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Sociaal Statuut"
      },
      "definition": {
        "nl": "Een statuut dat bepaalt welke sociale rechten een persoon geniet. "
      },
      "parents": [
        "https://data.vlaanderen.be/ns/schuldbeheer#Solvabiliteitsindicator"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Solvabiliteitsindicator",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Solvabiliteitsindicator"
      },
      "definition": {
        "nl": "Informatie die een aanwijzing geeft over de financiële draagkracht van een persoon."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Solvabiliteitsprofiel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Solvabiliteitsprofiel"
      },
      "definition": {
        "nl": "Categorisatie van de financiële draagkracht van een persoon."
      },
      "parents": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#ToerekeningVanBetaling",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Toerekening van Betaling"
      },
      "definition": {
        "nl": "Het toewijzen van een betaald geldbedrag aan een schuldvordering. "
      },
      "usage": {
        "nl": "Een expliciete modellering van een eventueel terugbetaling werd niet voorzien."
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Vermindering",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Vermindering"
      },
      "definition": {
        "nl": "Het verlagen van de omvang van de schuldvordering."
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Voorstel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Voorstel"
      },
      "definition": {
        "nl": "Het aanbieden van een afbetalingsplan."
      },
      "parents": [
        "http://www.w3.org/ns/prov#Activity"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/schuldbeheer#Wanbetalingsvergoeding",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Wanbetalingsvergoeding"
