  rdfs:domain <https://data.vlaanderen.be/ns/schuldbeheer#Wanbetalingsvergoeding>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/schuldbeheer>;
  rdfs:range schemas:MonetaryAmount .

<https://data.vlaanderen.be/ns/schuldbeheer#kwijtgescholdenBedrag> a owl:ObjectProperty;
  rdfs:label "kwijtgescholden bedrag"@nl;
