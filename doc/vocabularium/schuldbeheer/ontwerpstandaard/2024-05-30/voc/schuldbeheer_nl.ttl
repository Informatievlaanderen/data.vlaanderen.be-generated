  rdfs:comment "De laatst mogelijke datum waarop een betaling moet worden gedaan om te voorkomen dat er eventuele boetes, interesten of andere sancties worden opgelegd."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/schuldbeheer#Vordering>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/schuldbeheer>;
