    <rdfs:comment xml:lang="nl">Ondergrens van een klasse van waarden van meetpunten.</rdfs:comment>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/bodem-en-ondergrond#Fractiemetingwaarde"/>
    <rdfs:range rdf:resource="http://def.isotc211.org/iso19103/2005/UnitsOfMeasure#Measure"/>
    <rdfs:subPropertyOf rdf:resource="https://data.vlaanderen.be/ns/bodem-en-ondergrond#Meetreekswaarde.meetpunt"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/bodem-en-ondergrond#Meetreekswaarde.meetwaarde">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/bodem-en-ondergrond"/>
    <rdfs:label xml:lang="nl">meetwaarde</rdfs:label>
    <rdfs:comment xml:lang="nl">Waarde van het geobserveerdKenmerk.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Datatype is te substitueren door het datatype vd meetwaarde.</ns1:usageNote>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/bodem-en-ondergrond#Meetreekswaarde"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
  </owl:ObjectProperty>
