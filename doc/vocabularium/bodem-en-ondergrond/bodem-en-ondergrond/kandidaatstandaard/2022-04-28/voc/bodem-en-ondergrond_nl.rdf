        <rdfs:label xml:lang="nl">Maaiveld</rdfs:label>
        <rdfs:comment xml:lang="nl">Het grondoppervlak, waarvan de hoogte vaak opgegeven wordt ten opzichte van een nationaal nul-niveau.</rdfs:comment>
      </owl:Class>
    </rdfs:range>
  </owl:ObjectProperty>
  <owl:Class rdf:about="https://data.vlaanderen.be/ns/bodem-en-ondergrond#Boring">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/bodem-en-ondergrond"/>
    <rdfs:label xml:lang="nl">Boring</rdfs:label>
