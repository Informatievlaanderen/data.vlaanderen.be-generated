        <rdfs:subClassOf rdf:resource="http://def.isotc211.org/iso19156/2011/SpatialSamplingFeature#SF_SpatialSamplingFeature"/>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://def.isotc211.org/iso19103/2005/UnitsOfMeasure#Measure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/bodem-en-ondergrond#BO_RuimtelijkBemonsteringsobject.ligging">
    <rdfs:isDefinedBy rdf:resource="https://data.vlaanderen.be/ns/bodem-en-ondergrond"/>
    <rdfs:label xml:lang="nl">ligging</rdfs:label>
    <rdfs:comment xml:lang="nl">De ligging vh Bemonsteringsobject.</rdfs:comment>
    <ns1:usageNote xml:lang="nl">Naast de geometrie zijn bijkomende aanduidingen vd locatie soms nuttig, bv in het geval ve Boorgat in welke gemeente is geboord.</ns1:usageNote>
    <rdfs:domain rdf:resource="https://data.vlaanderen.be/ns/bodem-en-ondergrond#BO_RuimtelijkBemonsteringsobject"/>
    <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Location"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/bodem-en-ondergrond#BO_RuimtelijkBemonsteringsobject.maaiveld">
