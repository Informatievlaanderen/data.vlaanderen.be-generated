<https://data.vlaanderen.be/ns/grondboringen#monstertype> a owl:ObjectProperty;
  <http://purl.org/vocab/vann/usageNote> "Bv of het gespoeld, gestoken, gekernd is."@nl;
  rdfs:comment "Materiële vorm die het Grondmonster aanneemt."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/grondboringen#Grondmonster>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/grondboringen>;
