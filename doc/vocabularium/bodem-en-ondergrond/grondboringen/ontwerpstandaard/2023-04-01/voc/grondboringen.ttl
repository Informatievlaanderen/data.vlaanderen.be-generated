  rdfs:domain <http://def.isotc211.org/iso19156/2011/DomainSpecificSamplingFeatures#Borehole>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/grondboringen>;
  rdfs:range <https://data.vlaanderen.be/ns/grondboringen#Grondboring> .

<https://data.vlaanderen.be/ns/grondboringen#materiaal> a owl:ObjectProperty;
