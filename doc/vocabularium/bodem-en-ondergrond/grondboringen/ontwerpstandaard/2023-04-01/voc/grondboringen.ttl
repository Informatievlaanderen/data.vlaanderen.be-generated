  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/grondboringen>;
  rdfs:range <http://def.isotc211.org/iso19156/2011/DomainSpecificSamplingFeatures#Borehole> .

<https://data.vlaanderen.be/ns/grondboringen#startpunt> a owl:ObjectProperty;
  <http://purl.org/vocab/vann/usageNote> "Doorgaans het maaiveld maar andere mogelijkheden zijn bv bouwput, boortafel, dijk, bestaand boorgat"@nl;
  rdfs:comment "Aard van het oppervlak waarop de boorkraag is geplaatst."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/grondboringen#Boormethode>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/grondboringen>;
