  rdfs:range <https://data.vlaanderen.be/ns/grondboringen#Bekisting>,
    <https://data.vlaanderen.be/ns/grondboringen#Bekistinginterval> .

<https://data.vlaanderen.be/ns/grondboringen#boorder> a rdf:Property;
  rdfs:comment "Diegene die de Boring heeft uitgevoerd."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/grondboringen#Grondboring>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/grondboringen> .

<https://data.vlaanderen.be/ns/grondboringen#boordiameter> a owl:ObjectProperty;
  rdfs:comment "Verwijzing naar de beschrijving van de Boordiameter van het boorgat."@nl;
  rdfs:domain <http://def.isotc211.org/iso19156/2011/DomainSpecificSamplingFeatures#Borehole>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/grondboringen>;
  rdfs:range <https://data.vlaanderen.be/ns/grondboringen#Boorgatdiameter> .

<https://data.vlaanderen.be/ns/grondboringen#boormeester> a rdf:Property;
  rdfs:comment "Opzichter bij de Boring."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/grondboringen#Grondboring>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/grondboringen> .

<https://data.vlaanderen.be/ns/grondboringen#boringLengte> a owl:ObjectProperty;
