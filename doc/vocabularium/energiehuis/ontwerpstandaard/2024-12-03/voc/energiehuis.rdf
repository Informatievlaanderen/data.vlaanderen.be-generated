  <owl:ObjectProperty rdf:about="https://data.vlaanderen.be/ns/energiehuis#Adviesverlening.type">
    <rdfs:isDefinedBy>
      <owl:Ontology rdf:about="https://data.vlaanderen.be/ns/energiehuis">
        <dc:license>https://data.vlaanderen.be/id/licentie/modellicentie-gratis-hergebruik/v1.0</dc:license>
        <dc:license rdf:resource="https://data.vlaanderen.be/id/licentie/modellicentie-gratis-hergebruik/v1.0"/>
        <dc:mediator>
