  "properties": [
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#statistiektype",
      "@type": "http://www.w3.org/2002/07/owl#ObjectProperty",
      "label": {
        "nl": "statistiektype"
      },
      "definition": {
        "nl": "Het type statistiek dat gebruikt wordt."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ],
      "range": [
        "http://www.w3.org/2004/02/skos/core#Concept"
