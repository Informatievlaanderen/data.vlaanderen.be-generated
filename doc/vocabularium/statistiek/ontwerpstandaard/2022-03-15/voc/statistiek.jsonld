  "classes": [
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#CentraalMoment",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "CentraalMoment"
      },
      "definition": {
        "nl": "Beschrijft het moment van een dataset of variable, ten opzichte van het gemiddelde."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Moment"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#DiscreteProbabiliteit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "DiscreteProbabiliteit"
      },
      "definition": {
        "nl": "Beschrijft hoe waarschijnlijk het is dat een variable binnen een bepaalde categorie valt."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Kwantiel",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Kwantiel"
      },
      "definition": {
        "nl": "Beschrijft voor een gegeven niveau p (gelegen tussen 0 en 1), de waarde w van een variable zodat deze de dataset verdeelt in een deel met grootte p van waarden kleiner dan w, en een deel van grootte 1-p met waarden groter dan w."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Moment",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Moment"
      },
      "usage": {
        "nl": "Om expliciet te maken dat het om een centraal of niet-centraal moment gaat, kan gewerkt worden met de resp. subklassen van Moment."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#NietCentraalMoment",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "NietCentraalMoment"
      },
      "definition": {
        "nl": "Beschrijft het moment van een dataset of variable, ten opzichte van het nulpunt."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Moment"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Probabiliteit",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Probabiliteit"
      },
      "definition": {
        "nl": "Beschrijft hoe waarschijnlijk het is dat een variabele een bepaalde drempelwaarde (al dan niet) overschrijdt."
      },
      "parents": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ]
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Statistiek",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "label": {
        "nl": "Statistiek"
      },
      "definition": {
        "nl": "Een stuk informatie dat een kenmerk van een dataset of variable beschrijft."
      },
      "parents": []
    }
  ],
