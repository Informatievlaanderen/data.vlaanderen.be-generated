    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#orde",
      "@type": "http://www.w3.org/2002/07/owl#DatatypeProperty",
      "label": {
        "nl": "orde"
      },
      "definition": {
        "nl": "Een positief geheel getal"
      },
      "usage": {
        "nl": "Het (niet-centale) eerste-ordemoment komt overeen met het gemiddelde, het (centrale) tweede-ordemoment komt overeen met de variantie."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/statistiek#Moment"
      ],
      "range": [
        "http://www.w3.org/2001/XMLSchema#integer"
      ],
      "generalization": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#operator",
      "@type": "http://www.w3.org/2002/07/owl#ObjectProperty",
      "label": {
        "nl": "operator"
      },
      "definition": {
        "nl": "De operator die gebruikt wordt om de probabiliteit te berekenen."
      },
      "usage": {
        "nl": "Veel voorkomende operators zijn groter dan, kleiner dan, gelijk aan."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/statistiek#Probabiliteit"
      ],
      "range": [
        "http://www.w3.org/2004/02/skos/core#Concept"
      ],
      "generalization": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#niveau",
      "@type": "http://www.w3.org/2002/07/owl#DatatypeProperty",
      "label": {
        "nl": "niveau"
      },
      "definition": {
        "nl": "Het niveau van een kwantiel wordt gebruikt om de waarde van het kwantiel uit te drukken."
      },
      "usage": {
        "nl": "Het niveau ligt tussen 0 en 1."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/statistiek#Kwantiel"
      ],
      "range": [
        "http://www.w3.org/2001/XMLSchema#double"
      ],
      "generalization": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#drempelwaarde",
      "@type": "http://www.w3.org/2002/07/owl#DatatypeProperty",
      "label": {
        "nl": "drempelwaarde"
      },
      "definition": {
        "nl": "De drempelwaarde bepaalt de keuze om een geprojecteerde waarschijnlijkheid of scores toe te kennen of te berekenen."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/statistiek#Probabiliteit"
      ],
      "range": [
        "http://www.w3.org/2001/XMLSchema#double"
      ],
      "generalization": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#categorie",
      "@type": "http://www.w3.org/2002/07/owl#ObjectProperty",
      "label": {
        "nl": "categorie"
      },
      "definition": {
        "nl": "De categorie die gebruikt wordt om de discrete probabiliteit die gebruikt wordt te berekenen."
      },
      "usage": {
        "nl": "Voorbeelden om de discrete probabiliteiten te berekenen zijn binomiaal, Poisson en Bernoulli."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/statistiek#DiscreteProbabiliteit"
      ],
      "range": [
        "http://def.isotc211.org/iso19103/2005/RecordsAndClassMetadata#Any"
      ],
      "generalization": []
    },
    {
      "@id": "https://data.vlaanderen.be/ns/statistiek#Statistiek.waarde",
      "@type": "http://www.w3.org/2002/07/owl#ObjectProperty",
      "label": {
        "nl": "waarde"
      },
      "definition": {
        "nl": "De probabiliteit dat een variabele binnen de opgegeven categorie valt."
      },
      "usage": {
        "nl": "De waarde moet in het interval [0..1] liggen."
      },
      "domain": [
        "https://data.vlaanderen.be/ns/statistiek#Statistiek"
      ],
      "range": [
        "https://data.vlaanderen.be/ns/generiek#Waarde"
      ],
      "generalization": []
    },
    {
