@prefix dc: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

skos:Concept a rdfs:Class;
  rdfs:label "StatistiekType"@nl,
    "OperatorType"@nl .

<https://data.vlaanderen.be/ns/waterkwaliteit#StatistischeObservatie> a rdfs:Class;
  rdfs:label "StatistischeObservatie"@nl .

<https://data.vlaanderen.be/ns/statistiek#CentraalMoment> a owl:Class;
  rdfs:label "CentraalMoment"@nl;
  rdfs:comment "Beschrijft het moment van een dataset of variable, ten opzichte van het gemiddelde."@nl;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:subClassOf <https://data.vlaanderen.be/ns/statistiek#Moment> .

<https://data.vlaanderen.be/ns/statistiek#DiscreteProbabiliteit.waarde> a owl:DatatypeProperty;
  rdfs:label "waarde"@nl;
  <http://purl.org/vocab/vann/usageNote> "De waarde moet in het interval [0..1] liggen."@nl;
  rdfs:comment "De probabiliteit dat een variabele binnen de opgegeven categorie valt."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/statistiek#DiscreteProbabiliteit>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:range xsd:double .

<https://data.vlaanderen.be/ns/statistiek#NietCentraalMoment> a owl:Class;
  rdfs:label "NietCentraalMoment"@nl;
  rdfs:comment "Beschrijft het moment van een dataset of variable, ten opzichte van het nulpunt."@nl;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:subClassOf <https://data.vlaanderen.be/ns/statistiek#Moment> .

<https://data.vlaanderen.be/ns/statistiek#Probabiliteit.waarde> a owl:DatatypeProperty;
  rdfs:label "waarde"@nl;
  <http://purl.org/vocab/vann/usageNote> "De waarde moet in het interval [0..1] liggen."@nl;
  rdfs:comment "De probabiliteit dat een variabele binnen de opgegeven categorie valt."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/statistiek#Probabiliteit>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:range xsd:double .

<https://data.vlaanderen.be/ns/statistiek#Statistiek.waarde> a owl:ObjectProperty;
  rdfs:label "waarde"@nl;
  <http://purl.org/vocab/vann/usageNote> "De waarde moet in het interval [0..1] liggen."@nl;
  rdfs:comment "De probabiliteit dat een variabele binnen de opgegeven categorie valt."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/statistiek#Statistiek>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:range <https://data.vlaanderen.be/ns/generiek#Waarde> .

<https://data.vlaanderen.be/ns/statistiek#categorie> a owl:ObjectProperty;
  rdfs:label "categorie"@nl;
  <http://purl.org/vocab/vann/usageNote> "Voorbeelden om de discrete probabiliteiten te berekenen zijn binomiaal, Poisson en Bernoulli."@nl;
  rdfs:comment "De categorie die gebruikt wordt om de discrete probabiliteit die gebruikt wordt te berekenen."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/statistiek#DiscreteProbabiliteit>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:range <http://def.isotc211.org/iso19103/2005/RecordsAndClassMetadata#Any> .

<https://data.vlaanderen.be/ns/statistiek#drempelwaarde> a owl:DatatypeProperty;
  rdfs:label "drempelwaarde"@nl;
  rdfs:comment "De drempelwaarde bepaalt de keuze om een geprojecteerde waarschijnlijkheid of scores toe te kennen of te berekenen."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/statistiek#Probabiliteit>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:range xsd:double .

<https://data.vlaanderen.be/ns/statistiek#niveau> a owl:DatatypeProperty;
  rdfs:label "niveau"@nl;
  <http://purl.org/vocab/vann/usageNote> "Het niveau ligt tussen 0 en 1."@nl;
  rdfs:comment "Het niveau van een kwantiel wordt gebruikt om de waarde van het kwantiel uit te drukken."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/statistiek#Kwantiel>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:range xsd:double .

<https://data.vlaanderen.be/ns/statistiek#operator> a owl:ObjectProperty;
  rdfs:label "operator"@nl;
  <http://purl.org/vocab/vann/usageNote> "Veel voorkomende operators zijn groter dan, kleiner dan, gelijk aan."@nl;
  rdfs:comment "De operator die gebruikt wordt om de probabiliteit te berekenen."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/statistiek#Probabiliteit>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:range skos:Concept .

<https://data.vlaanderen.be/ns/statistiek#orde> a owl:DatatypeProperty;
  rdfs:label "orde"@nl;
  <http://purl.org/vocab/vann/usageNote> "Het (niet-centale) eerste-ordemoment komt overeen met het gemiddelde, het (centrale) tweede-ordemoment komt overeen met de variantie."@nl;
  rdfs:comment "Een positief geheel getal"@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/statistiek#Moment>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:range xsd:integer .

<https://data.vlaanderen.be/ns/statistiek#statistiektype> a owl:ObjectProperty;
  rdfs:label "statistiektype"@nl;
  rdfs:comment "Het type statistiek dat gebruikt wordt."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/statistiek#Statistiek>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:range skos:Concept .

<http://data.vlaanderen.be/ns/statistiek#StatistischeObservatie.resultaat> a rdf:Property;
  rdfs:label "resultaat"@nl .

<http://def.isotc211.org/iso19156/2011/Observation#OM_Observation.featureOfInterest> a rdf:Property;
  rdfs:label "geobserveerdObject"@nl .

<http://def.isotc211.org/iso19156/2011/Observation#OM_Observation.observedProperty> a rdf:Property;
  rdfs:label "geobserveerdKenmerk"@nl .

<http://def.isotc211.org/iso19156/2011/Observation#OM_Observation.phenomenonTime> a rdf:Property;
  rdfs:label "fenomeentijd"@nl .

<http://def.isotc211.org/iso19156/2011/Observation#OM_Observation.resultTime> a rdf:Property;
  rdfs:label "resultaattijd"@nl .

<https://data.vlaanderen.be/ns/statistiek#Kwantiel> a owl:Class;
  rdfs:label "Kwantiel"@nl;
  rdfs:comment "Beschrijft voor een gegeven niveau p (gelegen tussen 0 en 1), de waarde w van een variable zodat deze de dataset verdeelt in een deel met grootte p van waarden kleiner dan w, en een deel van grootte 1-p met waarden groter dan w."@nl;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:subClassOf <https://data.vlaanderen.be/ns/statistiek#Statistiek> .

<https://data.vlaanderen.be/ns/statistiek#DiscreteProbabiliteit> a owl:Class;
  rdfs:label "DiscreteProbabiliteit"@nl;
  rdfs:comment "Beschrijft hoe waarschijnlijk het is dat een variable binnen een bepaalde categorie valt."@nl;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:subClassOf <https://data.vlaanderen.be/ns/statistiek#Statistiek> .

<https://data.vlaanderen.be/ns/statistiek#Moment> a owl:Class;
  rdfs:label "Moment"@nl;
  <http://purl.org/vocab/vann/usageNote> "Om expliciet te maken dat het om een centraal of niet-centraal moment gaat, kan gewerkt worden met de resp. subklassen van Moment."@nl;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:subClassOf <https://data.vlaanderen.be/ns/statistiek#Statistiek> .

<https://data.vlaanderen.be/ns/statistiek#Probabiliteit> a owl:Class;
  rdfs:label "Probabiliteit"@nl;
  rdfs:comment "Beschrijft hoe waarschijnlijk het is dat een variabele een bepaalde drempelwaarde (al dan niet) overschrijdt."@nl;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek>;
  rdfs:subClassOf <https://data.vlaanderen.be/ns/statistiek#Statistiek> .

<https://data.vlaanderen.be/ns/statistiek#Statistiek> a owl:Class;
  rdfs:label "Statistiek"@nl;
  rdfs:comment "Een stuk informatie dat een kenmerk van een dataset of variable beschrijft."@nl;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/statistiek> .

