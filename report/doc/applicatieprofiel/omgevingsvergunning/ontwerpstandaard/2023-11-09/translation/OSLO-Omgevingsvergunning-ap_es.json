    },
    {
      "EA-Guid": "{6C491CB9-6611-49af-9A8B-371C84D9E43D}",
      "name": "Ruimtelijke Eenheid",
      "label": {
        "nl": "Ruimtelijke Eenheid",
        "es": "Enter your translation here"
      },
      "definition": {
        "nl": "Eén gebied (of meerdere gebieden) van land en/of water, of één enkel volume (of meerdere volumes) ruimte, afgebakend voor een bepaald doel.",
        "es": "Enter your translation here"
      },
      "usage": {
        "nl": "Definitie conform ISO 19152 - Land Administration Domain Model.",
        "es": "Enter your translation here"
      }
    }
